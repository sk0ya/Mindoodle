      <stop offset="100%" style="stop-color:#e3f2fd;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background with rounded corners -->
  <rect width="180" height="180" rx="28" fill="url(#brainGradient)" stroke="#1565c0" stroke-width="2"/>
  
